{
  "name": "@pommejs/server",
  "version": "2.0.0",
  "description": "PommeJS Server is a library to build routes for your API with Express.JS, ref: https://github.com/evertonbzr/pomme-ts",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch ./_code.ts",
    "test": "NODE_ENV=test vitest",
    "publish-package": "tsc && npm publish"
  },
  "keywords": [
    "typescript",
    "router",
    "express",
    "api",
    "builder"
  ],
  "author": "Everton Fernandes",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.4.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.4"
  }
}
